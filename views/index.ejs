<%- include("partials/header.ejs") %>
<h1>Zach Monroe's Blog</h1>
<% if (locals.posts.length > 0) { %>
<%  for (var i = 0; i < posts.length; i++){ %>
  <div class= "container">
    <h2> <%= posts[i]["title"] %> </h2>
    <h4> <%= posts[i]["author"] %> </h4>
    <p> <%= posts[i]["post"] %> </p>
  </div>
 <% } %> 
<% } else { %>
  <h2>No Posts - Time to get creative!</h2>
<% } %>
<%- include("partials/footer.ejs") %>


